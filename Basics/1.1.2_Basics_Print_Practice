import Foundation

//The following print statements will make various patterns

print("Hello World")
print(" ")
print("p")
print("  p")
print("    p")
print("  p")
print("p")
print(" ")
print("x   x")
print("  x")
print("x   x")

//The next three lines show that numbers can be printed.
print(99)
print(55)
print(87)

